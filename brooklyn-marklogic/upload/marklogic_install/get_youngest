instance_id=`/opt/aws/bin/ec2-metadata -i | awk '{print $2}'`
asg=`/opt/aws/bin/ec2-describe-instances -O AKIAJFEVG7RSZONJC57Q  -W Ar13VsmVJkXVNYySc/heDES4ac4+efrMcfiSqhTX $instance_id  | awk  '{for (i=1;i<=NF;i++){if ($i == "aws:autoscaling:groupName")printf "%s\n",$(i+1)}}'`
/opt/aws/bin/ec2-describe-instances -O AKIAJFEVG7RSZONJC57Q  -W Ar13VsmVJkXVNYySc/heDES4ac4+efrMcfiSqhTX -F tag:aws:autoscaling:groupName=$asg -F instance-state-code=16  -F instance-state-code=0 | awk '/INSTANCE/ {split($10,tim,"+");sysstring=sprintf("date +\x25\x25s -d \"%s\"",tim[1]);sysstring | getline ids;printf "%s %s\n", $4,ids}' | sort -k 2 | head -1 | awk '{print $1}'
